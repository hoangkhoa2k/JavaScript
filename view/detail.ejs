        }
        
        div.stars {
            display: table;
        }
        
        input.star {
            display: none;
        }
        
        label.star {
            float: right;
            padding: 10px;
            font-size: 36px;
            color: #444;
            font-weight: normal;
            transition: all .2s;
        }
        
        input.star:checked~label.star:before {
            content: '\f005';
            color: #FD4;
            transition: all .25s;
        }
        
        input.star-5:checked~label.star:before {
            color: yellow;
        }
        
        input.star-1:checked~label.star:before {
            color: red;
        }
        
        input.star-2:checked~label.star:before {
            color: rgb(255, 75, 75);
        }
        
        input.star-3:checked~label.star:before {
            color: #F62;
        }
        
        input.star-4:checked~label.star:before {
            color: rgb(255, 185, 34);
        }
        
        label.star:hover {
            transform: rotate(-15deg) scale(1.3);
        }
        
        label.star:before {
            content: '\f006';
            font-family: FontAwesome;
        }
        
        h4.media-heading.text-uppercase.reviews {
            font-weight: bold;
            color: #7bbd42;
        }
        
        img.media-object.img-circle {
            width: 70%;
        }
        
        a.pull-left {
            width: 100px;
        }
        
        .well-lg .col-md-12,
        .col-md-2 {
            padding: 0;
            min-width: 70px;
        }
        
        p.media-comment {
            margin: 40px 0 5px 0;
        }
        
        .actions {
            display: contents;
        }
        
        .col-md-12.rating-box {
            float: right;
        }
        
        .like.button.btn-cart {
            border-radius: 10px;
            background: #2b2bff !important;
        }
    </style>
